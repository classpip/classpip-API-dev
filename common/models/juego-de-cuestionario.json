{
  "name": "JuegoDeCuestionario",
  "plural": "JuegosDeCuestionario",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "NombreJuego": {
      "type": "string",
      "required": true
    },
    "Modo": {
      "type": "string",
      "required": true
    },
    "Modalidad": {
      "type": "string",
      "required": true
    },
    "PuntuacionCorrecta": {
      "type": "number",
      "required": true
    },
    "PuntuacionIncorrecta": {
      "type": "number",
      "required": true
    },
    "TiempoLimite": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "Presentacion": {
      "type": "string",
      "required": true
    },
    "JuegoActivo": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "JuegoTerminado": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "alumnos": {
      "type": "hasMany",
      "model": "Alumno",
      "foreignKey": "juegoDeCuestionarioId",
      "options": {
        "nestRemoting": true
      },
      "through": "AlumnoJuegoDeCuestionario"
    },
    "equipos": {
      "type": "hasMany",
      "model": "Equipo",
      "foreignKey": "juegoDeCuestionarioId",
      "options": {
        "nestRemoting": true
      },
      "through": "EquipoJuegoDeCuestionario"
    }
  },
  "acls": [],
  "methods": {}
}
